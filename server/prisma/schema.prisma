// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Payroll {
  cycle_id Int     @id @default(autoincrement())
  cycle_title String @unique
  cycle_base_salary_type String?
  cycle_description String
}

model Payroll_addition {
  payroll_addition_id Int @id @default(autoincrement())
}

model Payroll_deduction {
  payroll_deduction_id Int @id @default(autoincrement())
}

model Payroll_overtime {
  payroll_ot_id Int @id @default(autoincrement())
}